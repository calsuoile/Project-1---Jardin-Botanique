olor: rgb(205, 248, 196);\n  opacity: 0.9;\n  position:absolute;\n  top: 5%;\n  left: 40%;\n  right: 30%;\n  transform: translate(50%,50%);\n}\n\nnav{\n  background: #d2f5e3;\n  display: flex;\n}\n\nnav.fixed {\n  position: fixed;\n  top: 0;\n}\n\nnav li {\n  list-style: none;\n  width: 100%;\n}\n\nnav li a {\n  display: block;\n  background: #94b4a4;\n  padding: 2em;\n  text-align: center;\n  box-shadow: 1px 1px 0 #0da;\n  color: black;\n  \n}\n\nnav li a:hover {\n  background: #0DA;\n}\n\n\n\n\n/*activities*/\n.activities {\n  text-align: center;\n}\n\n.article {\n  display: flex;\n  margin-top: 20px;\n  margin-left: 25px;\n  padding: 20px 20px; \n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  background-color: #3eb98a;\n  display: none;\n  border: transparent;\n  border-radius: 30px;\n  width: 90%;\n  justify-content: space-between;\n}\n\n.openActivities:hover{\n  background-color:  #0DA;\n  transform: scale(1.5);\n}\n\n.closeActivities {\n  padding: 10px 20px;\n  background-color: #94b4a4;\n  color: #fff;\n  border: transparent;\n  border-radius: 40px;\n  font-size: 1.7rem;\n  width: 80px;\n  font-size: medium;\n  margin: 10px;\n}\n\n.openActivities {\n  padding: 10px 20px;\n  background-color:  #94b4a4;\n  color: #fff;\n  border: transparent;\n  border-radius: 40px;\n  box-shadow: 10px 5px 5px rgb(80, 94, 88);\n  font-size: 1.7rem;\n  width: 200px;\n  height: 85px;\n  margin-top: 40px;\n  margin-bottom: 50px;\n}\n\n#whatToDo {\n  align-items: baseline;\n  display: flex;\n  flex-flow: row, wrap;\n  justify-content: space-around;\n  margin-top: 40px;\n  margin-bottom: 40px;\n}\n/*activities*/\n\n/* presentation */\n.bloc {\n  margin: 100px 0 100px 0;\n}\n\n.contener {\n  display: flex;\n  justify-content: space-around;\n  background-color: #d2f5e3;\n  margin: 20px;\n}\n\n.contener-box1 {\n  width: 40%;\n  margin: 20px;\n}\n\n.contener-box1 img {\n  width: 100%;\n}\n\n.contener-box2 {\n  display: flex;\n  flex-direction: column;\n  width: 50%;\n  margin-right: 30px;\n  justify-content: space-between;\n}\n\n.contener-box2-element-1 {\n  font-size: 1.5rem;\n  text-align: justify;\n  line-height: 1.8;\n  letter-spacing: 1.5px;\n  margin: 15px;\n}\n\n.contener-box2-element-2 {\n  background-color: #94b4a4;\n  font-size: 50px;\n  font-style: italic;\n  margin: 15px;\n  text-align: center;\n  box-sizing: content-box;\n}\n\n/* d√©but mise en forme formulaire booking */\n.booking, .form {\n  margin: 50px 10px 50px 10px;\n}\n\nhr {\n  border-color: #d2f5e3;\n}\n\nfieldset {\n  margin-bottom: 15px;\n  padding: 10px; \n  width: 50%;\n  border-color: #94b4a4;\n  margin: 2px;\n\n}\n\n.reservation {\n  display: flex;\n}\n\n.formulaire {\n  display: flex;\n  justify-content: center;\n}\n\n.bouton-booking {\n  display: flex;\n  justify-content: center;\n}\n\n.bouton-form {\n  display: flex;\n  justify-content: center;\n}\n\nlegend {\n  padding: 0px 3px;\n  font-weight: bold;\n  font-variant: small-caps;\n}\n \nlabel {\n  width: 110px;\n  display: inline-block;\n  vertical-align: top;\n  margin: 6px;\n  \n}\n \ninput:focus {\n  background: #eaeaea;\n}\n \ninput, textarea {\n  width: 249px;\n\n}\n \ntextarea {\n  height: 100px;\n}\n \nselect {\n  width: 254px;\n}\n \ninput[type=submit] {\n  width: 100px;\n  padding: 10px;\n  background-color:#94b4a4;\n  color: #fff;\n  border-width: thin;\n  box-shadow: 1px 1px 0 #0DA;\n  border-radius: 3px;  \n}\n\ninput[type=submit]:hover {\n  background: #0DA;\n}\n\n/* fin mise en forme formulaire booking */\n\n/* team */\n.team-content-element {\n  display: flex;\n  padding: 20px;\n}\n\n.team-content-element-item {\n  display: flex;\n  text-align: center;\n  margin: auto;\n  max-width: 40rem;\n  max-height: 30rem;\n  background-color: #94b4a4;\n  border-radius:  2rem 2rem;\n}\n\n.team-content-element-item img {\n  border-radius: 2rem 0 0 2rem;\n  min-height: 10rem;\n}\n\n.team-content-element-item-group {\n  padding: 1rem;\n  width: 20rem;\n  margin: auto;\n}\n\n.team-content-element-item figure figcaption {\n  padding: 4rem 0;\n}\n\n.team-content-element-name {\n  margin-top: 2rem;\n}\n\n/* fin team */"},"lineCount":305}},"error":null,"hash":"78d0472830f950e057952f3c3c85ec28","cacheData":{"env":{}}}